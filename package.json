{
  "name": "angular-bluebird-promises",
  "version": "1.0.1",
  "description": "Replaces $q with bluebirds promise API",
  "main": "dist/angular-bluebird-promises.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattlewis92/angular-bluebird-promises.git"
  },
  "keywords": [
    "angular",
    "bluebird",
    "promise"
  ],
  "author": "Matt Lewis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattlewis92/angular-bluebird-promises/issues"
  },
  "homepage": "https://github.com/mattlewis92/angular-bluebird-promises",
  "devDependencies": {
    "angular-mocks": ">=1.2.0",
    "babel-core": "~6.3.15",
    "babel-loader": "~6.2.0",
    "babel-plugin-add-module-exports": "~0.1.1",
    "babel-polyfill": "~6.3.14",
    "babel-preset-es2015": "~6.3.13",
    "conventional-changelog": "~0.5.1",
    "eslint": "~1.10.1",
    "eslint-config-mwl": "~0.3.0",
    "eslint-loader": "~1.1.0",
    "karma": "~0.13.9",
    "karma-chai-plugins": "~0.6.0",
    "karma-mocha": "~0.2.0",
    "karma-phantomjs-launcher": "~0.2.0",
    "karma-sourcemap-loader": "~0.3.5",
    "karma-webpack": "~1.7.0",
    "mocha": "~2.3.2",
    "ng-annotate-loader": "0.0.10",
    "node-libs-browser": "~0.5.2",
    "open": "0.0.5",
    "phantomjs": "~1.9.17",
    "pre-git": "~3.1.1",
    "webpack": "~1.12.2",
    "webpack-dev-server": "~1.14.0"
  },
  "peerDependencies": {
    "angular": ">=1.2.0",
    "bluebird": ">=2.0.0"
  },
  "scripts": {
    "test:watch": "karma start --watch",
    "test": "karma start",
    "build:unmin": "webpack --config webpack.config.build.js",
    "build:min": "webpack -p --config webpack.config.build.js",
    "build": "npm run build:unmin && npm run build:min",
    "start": "webpack-dev-server",
    "commit": "commit-wizard",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w"
  },
  "config": {
    "pre-git": {
      "commit-msg": "conventional"
    }
  }
}
